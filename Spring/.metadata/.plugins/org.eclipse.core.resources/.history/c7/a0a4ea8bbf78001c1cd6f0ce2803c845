package br.org.generation.mentalidades.controller;

import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;
import org.springframework.web.bind.annotation.GetMapping;

@RestController
@RequestMapping("/mentalidades")
public class MentalidadesController {
	
	@GetMapping
	public String Mentalidades() {
		return "As habilidades e mentalidades que utilizei para fazer essa atividade foram: Atenção aos Detalhes, Persistência e Orientação ao Futuro";
	}

}
